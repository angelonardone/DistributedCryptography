<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="Current">
	
	
	<PropertyGroup>
		<AssemblyName>hsm.rest</AssemblyName>
		<TargetFramework>$(TargetFramework)</TargetFramework>
		<OutputType>Library</OutputType>
		<GenerateDependencyFile>false</GenerateDependencyFile>
		<CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
		<PostgresqlSupport>false</PostgresqlSupport>
		<OfficeSupport>false</OfficeSupport>
		<MailSupport>false</MailSupport>
		<GrpcClientSupport>false</GrpcClientSupport>
		<PdfLibrary></PdfLibrary>
		<SAPErpSupport>false</SAPErpSupport>
		<PDFSupport>false</PDFSupport>
		<SearchSupport>false</SearchSupport>
	</PropertyGroup>

	<ItemGroup>

        <ProjectReference Include="..\GeneXus.Programs.Common\GeneXus.Programs.Common.csproj" >
            <OutputItemType>$(CommonOutputItemType)</OutputItemType>
        </ProjectReference>

	</ItemGroup>

	<ItemGroup>

        <Reference Include="GeneXus" >
            <HintPath>$(WebBinDirectory)\GeneXus.dll</HintPath>
        </Reference>
        <Reference Include="Google.Authenticator" >
            <HintPath>$(WebBinDirectory)\Google.Authenticator.dll</HintPath>
        </Reference>
        <Reference Include="GxInternetLib" >
            <HintPath>$(WebBinDirectory)\GxInternetLib.dll</HintPath>
        </Reference>
        <Reference Include="DistricutedCryptographyLib" >
            <HintPath>$(WebBinDirectory)\DistricutedCryptographyLib.dll</HintPath>
        </Reference>

	</ItemGroup>

	<ItemGroup>
		<Compile Include = "$(WebDirectory)\hsm\rest_services.cs" />
		<Compile Include = "$(WebDirectory)\hsm\signschnorr.cs" />
		<Compile Include = "$(WebDirectory)\hsm\encrypttopubkey.cs" />
		<Compile Include = "$(WebDirectory)\hsm\encrypt.cs" />
		<Compile Include = "$(WebDirectory)\hsm\rest.cs" />
		<Compile Include = "$(WebDirectory)\hsm\getpubkey.cs" />
		<Compile Include = "$(WebDirectory)\hsm\decrypt.cs" />
		<Compile Include = "$(WebDirectory)\hsm\sign.cs" />

	</ItemGroup>

	<Import Project="..\DotNetCoreBaseProject.targets"/>	
</Project>

